<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Morgan">

    <Color x:Key="BackgroundDark">#252525</Color>
    <SolidColorBrush x:Key="BackgroundDarkBrush" Color="{StaticResource BackgroundDark}"/>
    

    <Color x:Key="GreenTertiary">#448e32</Color>
    <SolidColorBrush x:Key="GreenTertiaryBrush" Color="{StaticResource GreenTertiary}"/>

    <Color x:Key="GreenSecondary">#01705e</Color>
    <SolidColorBrush x:Key="GreenSecondaryBrush" Color="{StaticResource GreenSecondary}"/>

    <Color x:Key="GreenPrimary">#01917a</Color>
    <SolidColorBrush x:Key="GreenPrimaryBrush" Color="{StaticResource GreenPrimary}"/>


    <Color x:Key="ForegroundLight">#f4f4f4</Color>
    <SolidColorBrush x:Key="ForegroundLightBrush" Color="{StaticResource ForegroundLight}"/>

    <Color x:Key="ForegroundDark">#333333</Color>
    <SolidColorBrush x:Key="ForegroundDarkBrush" Color="{StaticResource ForegroundDark}"/>

</ResourceDictionary>